{
  "description": "Demand Accountability. Automatically tags social media profiles based on their past offenses.",
  "manifest_version": 2,
  "name": "prairiewatch.dog",
  "version": "1.0.0",
  "homepage_url": "https://prairiewatch.dog",
  "icons": {
    "48": "icons/star.png"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content.css"],
      "js": ["js/jquery-3.5.1.slim.min.js", "test.js"]
    }
  ]
}